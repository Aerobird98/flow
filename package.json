{
  "name": "flow",
  "version": "0.14.10",
  "description": "A compact web based rich-text editor that builds on top of Slate-js with powerful, yet simple and accessible editing capabilities.",
  "private": true,
  "scripts": {
    "build-development": "esbuild index.jsx --bundle --outdir=build --jsx-factory=h --jsx-fragment=Fragment --minify --sourcemap --define:process.env.NODE_ENV='\"development\"'",
    "build-production": "esbuild index.jsx --bundle --outdir=build --jsx-factory=h --jsx-fragment=Fragment --minify --sourcemap --define:process.env.NODE_ENV='\"production\"'",
    "deploy": "gh-pages -d build",
    "release": "npm run build-production && npm run deploy"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Aerobird98/flow.git"
  },
  "author": "Aerobird98",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Aerobird98/flow/issues"
  },
  "homepage": "http://Aerobird98.github.io/flow",
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^1.2.34",
    "@fortawesome/free-solid-svg-icons": "^5.15.2",
    "@fortawesome/react-fontawesome": "^0.1.14",
    "preact": "^10.5.11",
    "slate": "^0.59.0",
    "slate-history": "^0.59.0",
    "slate-react": "^0.59.0",
    "theme-ui": "^0.3.5"
  },
  "devDependencies": {
    "esbuild": "^0.8.36",
    "gh-pages": "^3.1.0",
    "prettier": "^2.2.1"
  }
}
